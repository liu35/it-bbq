AA;AACA;AA7BA,CAAA","sourcesContent":["<template>\r\n    <div class=\"mcontaner\">\r\n      <Header></Header>\r\n  \r\n      <div class=\"block\">\r\n        <el-timeline>\r\n          <el-timeline-item :timestamp=\"post.createTime\" placement=\"top\" v-for=\"post in posts\">\r\n            <el-card>\r\n              <el-tag size=\"mini\" type=\"success\" v-show=\"post.top==1\">top</el-tag>\r\n              <el-tag size=\"mini\" type=\"success\" v-show=\"post.marrow==1\">hot</el-tag>\r\n              <el-tag size=\"mini\" type=\"success\" v-show=\"post.official==1\">official</el-tag>\r\n              <h2 style=\"text-align: center\">\r\n                <router-link :to=\"{name: 'PostsDetail', params: {postId: post.id}}\">\r\n                  {{post.title}}\r\n                </router-link>\r\n              </h2>\r\n              <div style=\"text-align: center\">\r\n                <span class=\"el-icon-user\" >{{post.authorName}}</span>\r\n                <div style=\"text-align: right\"><el-tag size=\"mini\">{{post.type}}</el-tag></div>\r\n              </div>\r\n              <p>{{post.htmlContent}}</p >\r\n              <div style=\"text-align:right\">\r\n              <p class=\"el-icon-thumb\">{{post.approvals}}</p >&emsp;&emsp;&emsp;\r\n              <p class=\"el-icon-view\">{{post.views}}</p >\r\n            </div>\r\n            </el-card>\r\n          </el-timeline-item>\r\n  \r\n        </el-timeline>\r\n  \r\n        <el-pagination class=\"mpage\"\r\n                       background\r\n                       layout=\"prev, pager, next\"\r\n                       :current-page=\"currentPage\"\r\n                       :page-size=\"pageSize\"\r\n                       :total=\"total\"\r\n                       @current-change=page>\r\n        </el-pagination>\r\n  \r\n      </div>\r\n  \r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n    import Header from \"../components/Header\";\r\n  \r\n    export default {\r\n      name: \"PostIndexSearch\",\r\n      components: {Header},\r\n      data() {\r\n        return {\r\n          posts: {},\r\n          currentPage: 1,\r\n          total: 0,\r\n          pageSize: 10\r\n        }\r\n      },\r\n      methods: {\r\n        page(currentPage) {\r\n          const _this = this\r\n  \r\n          const search = _this.$route.params.search;\r\n  \r\n          _this.$axios.get(\"/posts/index/list?pageNum=\" + currentPage + \"&search=\" +  search).then(res => {\r\n            console.log(res)\r\n            _this.posts = res.data.data.records\r\n            _this.currentPage = res.data.data.current\r\n            _this.total = res.data.data.total\r\n            _this.pageSize = res.data.data.size\r\n  \r\n          })\r\n        }\r\n      },\r\n      created() {\r\n        this.page(1)\r\n      }\r\n    }\r\n  </script>\r\n  \r\n  <style scoped>\r\n  \r\n    .mpage {\r\n      margin: 0 auto;\r\n      text-align: center;\r\n    }\r\n  \r\n  </style>"],"sourceRoot":"src/views"}]}