;;AACA,WAAA,MAAA,CAAA,GAAA,CAAA,gBAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,OAFA;AAGA,KAxBA;AAyBA,IAAA,aAzBA,yBAyBA,MAzBA,EAyBA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,iBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,MAAA,EAAA;AAAA;AAAA,OAAA;AACA,KA3BA;AA4BA,IAAA,MA5BA,oBA4BA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,iBAAA;AAAA,QAAA,MAAA,EAAA;AAAA,UAAA,MAAA,EAAA,KAAA;AAAA;AAAA,OAAA;AACA;AA9BA,GAfA;AA+CA,EAAA,OA/CA,qBA+CA;AAEA,SAAA,WAAA;;AACA,QAAA,KAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,QAAA,EAAA;AACA,WAAA,IAAA,CAAA,QAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,QAAA;AACA,WAAA,IAAA,CAAA,MAAA,GAAA,KAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,QAAA,GAAA,IAAA;;AACA,UAAA,KAAA,MAAA,CAAA,OAAA,CAAA,OAAA,CAAA,IAAA,IAAA,OAAA,EAAA;AACA,aAAA,OAAA,GAAA,IAAA;AACA;AAEA;AAEA,GA5DA;AA6DA,EAAA,KAAA,EAAA;AACA,IAAA,MAAA,EAAA;AACA,MAAA,OAAA,EAAA,iBAAA,GAAA,EAAA,MAAA,EAAA;AACA,YAAA,GAAA,IAAA,MAAA,EAAA;AACA,eAAA,OAAA,CAAA,EAAA,CAAA,CAAA;AACA;AACA,OALA;AAMA,MAAA,IAAA,EAAA;AANA;AADA;AA7DA,CAAA","sourcesContent":["<template>\r\n  <div class=\"m-content\">\r\n    <h2 style=\"text-align: center;\">Welcome to the BBQ forum</h2>\r\n\r\n    <div class=\"block\">\r\n      <div style=\"text-align: left\" v-show=\"isAdmin\">\r\n        <el-button type=\"warning\" @click=\"admin\" size=\"medium\">Admin</el-button>\r\n      </div>\r\n\r\n      <span v-show=\"!hasLogin\">\r\n        <el-button type=\"primary\" @click=\"login\" size=\"small\">Login</el-button>\r\n      </span>\r\n\r\n      <span v-show=\"hasLogin\">\r\n        <el-button type=\"danger\" @click=\"logout\" size=\"small\">LogOut</el-button>\r\n      </span>\r\n      <br><br>\r\n\r\n      <el-avatar :size=\"60\" :src=\"user.avatar\"></el-avatar>\r\n      <div>{{ user.username }}</div>\r\n    </div>\r\n\r\n    <div class=\"maction\">\r\n\r\n\r\n      <el-menu default-active=\"activeIndex\" class=\"el-menu-demo\" mode=\"horizontal\" background-color=\"#635f5e\" text-color=\"#FFFFFF\" >\r\n        <el-menu-item index=\"1\">\r\n          <span>\r\n            <el-link href=\"/posts/index\">All Posts</el-link>\r\n          </span>\r\n        </el-menu-item>\r\n\r\n        <el-submenu index=\"2\">\r\n          \r\n         <template slot=\"title\">CATEGORY</template>\r\n          <el-menu-item index=\"2-index\" v-for=\"(item, index) in typeList\" :key=\"index\" @click=\"getPostByType(item.id)\">\r\n              {{item.name}}\r\n          </el-menu-item>\r\n        </el-submenu>\r\n\r\n\r\n        <el-menu-item index=\"3\">\r\n          <span>\r\n            <el-link type=\"success\" href=\"/posts/index/add\">\r\n              CREATE POST\r\n            </el-link>\r\n          </span>\r\n        </el-menu-item>\r\n\r\n        <el-menu-item index=\"4\">\r\n          <span>\r\n            <el-link type=\"success\" href=\"/user/index\">\r\n              USER CENTER\r\n            </el-link>\r\n          </span>\r\n        </el-menu-item>\r\n\r\n\r\n        <div style=\"margin-left: 800px;margin-top: 12px;\">\r\n          <el-input size=\"small\" style=\"width: 200px;\"  @keydown.enter.native=\"search()\" v-model=\"searchData\"></el-input>\r\n          <el-button slot=\"append\" icon=\"el-icon-search\" @click=\"search()\" size=\"small\" style=\"margin-left: 10px\"></el-button>\r\n        </div>\r\n        \r\n\r\n      </el-menu>\r\n    </div>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Header\",\r\n  data() {\r\n    return {\r\n      user: {\r\n        username: 'please login',\r\n        avatar: 'https://img2.baidu.com/it/u=2306449352,310199388&fm=253&fmt=auto&app=138&f=PNG?w=500&h=343'\r\n      },\r\n      hasLogin: false,\r\n      isAdmin: false,\r\n      activeIndex: '1',\r\n      typeList: [],\r\n      searchData: ''\r\n    };\r\n  },\r\n  methods: {\r\n    login() {\r\n      this.$router.push({ name: 'Login' })\r\n    },\r\n    logout() {\r\n      const _this = this\r\n      _this.$axios.get(\"/logout\", {\r\n        headers: {\r\n          \"Authorization\": localStorage.getItem(\"token\")\r\n        }\r\n      }).then(res => {\r\n        _this.$store.commit(\"REMOVE_INFO\")\r\n        _this.$router.push({ name: 'PostsIndex' }).catch(() => {})\r\n        _this.$router.go(0)\r\n\r\n      })\r\n    },\r\n    admin(){\r\n      this.$router.push({ name: 'Admin' })\r\n    },\r\n    getTypeList() {\r\n      this.$axios.get(\"/article/types\").then(res => {\r\n        this.typeList = res.data.data\r\n      })\r\n    },\r\n    getPostByType(typeId) {\r\n       this.$router.push({name : 'PostIndexByType', params : {typeId : typeId}})\r\n    },\r\n    search() {\r\n       this.$router.push({name : 'PostIndexSearch', params : {search: this.searchData}})\r\n    }\r\n  },\r\n  created() {\r\n\r\n    this.getTypeList();\r\n    if (this.$store.getters.getUser.username) {\r\n      this.user.username = this.$store.getters.getUser.username\r\n      this.user.avatar = this.$store.getters.getUser.avatar\r\n      this.hasLogin = true\r\n      if (this.$store.getters.getUser.role == \"ADMIN\") {\r\n        this.isAdmin = true\r\n      }\r\n      \r\n    }\r\n\r\n  },\r\n  watch: {\r\n    $route: {\r\n    handler: function(val, oldVal){\r\n      if (val != oldVal) {\r\n        this.$router.go(0)\r\n      }\r\n    },\r\n    deep: true\r\n  }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.m-content {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  text-align: left;\r\n}\r\n\r\n.maction {\r\n  margin: 10px 0;\r\n  text-align: center;\r\n}\r\n\r\n.block {\r\n  text-align: right;\r\n}\r\n</style>"],"sourceRoot":"src/components"}]}